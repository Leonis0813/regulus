<tr class='<%= state_to_class(prediction.state) %>'>
  <td>
    <%= time_to_string(prediction.created_at) %>
  </td>
  <td>
    <%= prediction.model %>
  </td>
  <td>
    <%=
      if prediction.from and prediction.to
        period_to_string(prediction.from, prediction.to)
      end
    %>
  </td>
  <td>
    <span class='glyphicon <%= icon_class(prediction) %>' style='color: <%= icon_color(prediction) %>'/>
  </td>
</tr>
